{
  "welcomeFile": "app/index.html",
  "routes": [
    {
      "source": "^/app/(.*)$",
      "target": "$1",
      "localDir": ".",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/appconfig/",
      "localDir": ".",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "destination": "srv-api",
      "csrfProtection": true
    },
    {
      "welcomeFile": "/app/risks/webapp/index.html",
      "routes": [
        {
          "source": "^/app/(.*)$",
          "localDir": "./",
          "target": "$1"
        },
        {
          "source": "^/service/(.*)$",
          "destination": "srv-api"
        }
      ]
    }
    
  ]
